{
  "$schema": "https://turborepo.dev/schema.json",
  "ui": "tui",
  "globalEnv": ["npm_config_user_agent"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "pkg:dev": {
      "cache": false,
      "persistent": true
    },
    "//#fmt": {},
    "//#fmt:fix": {
      "cache": false
    },
    "//#lint": {},
    "//#lint:fix": {
      "cache": false
    }
  }
}
