{
  "name": "@mayrlabs/prunejs",
  "version": "0.0.3",
  "private": false,
  "description": "Scan JS/TS projects and detects unused files, functions, classes and exports",
  "keywords": [
    "unused code",
    "dead code",
    "tree shaking",
    "analysis",
    "typescript",
    "javascript",
    "cleanup",
    "refactoring",
    "maintainability",
    "exports",
    "imports"
  ],
  "homepage": "https://github.com/MayR-Labs/mayrlabs-js/tree/main/packages/prunejs#readme",
  "bugs": {
    "url": "https://github.com/MayR-Labs/mayrlabs-js/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Aghogho Meyoron",
    "email": "youngmayor.dev@gmail.com",
    "url": "https://mayrlabs.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MayR-Labs/mayrlabs-js.git",
    "directory": "packages/prunejs"
  },
  "bin": {
    "prunejs": "dist/cli.cjs"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "type": "commonjs",
  "main": "dist/cli.cjs",
  "module": "dist/cli.mjs",
  "exports": {
    ".": {
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "lint": "eslint .",
    "check": "npm run format && npm run lint && npm run test && npm pkg fix",
    "demo": "tsx src/cli.ts",
    "demo:dist": "node dist/cli.cjs",
    "build": "tsdown",
    "pkg:dev": "tsdown --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "figlet": "^1.10.0",
    "fs-extra": "^11.2.0",
    "ignore": "^7.0.5",
    "inquirer": "^8.2.7",
    "ora": "^5.4.1",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@mayrlabs/core": "*",
    "@repo/eslint-config": "^0.0.0",
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.1",
    "eslint-plugin-node": "^11.1.0",
    "mock-fs": "^5.5.0",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.18"
  }
}
