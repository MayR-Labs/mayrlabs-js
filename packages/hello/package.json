{
  "name": "@mayrlabs/hello",
  "version": "1.0.1",
  "private": false,
  "description": "A simple hello world for MayR Labs",
  "keywords": [
    "Labs",
    "MayR"
  ],
  "homepage": "https://github.com/MayR-Labs/mayrlabs-js/tree/main/packages/hello#readme",
  "bugs": {
    "url": "https://github.com/MayR-Labs/mayrlabs-js/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Aghogho Meyoron",
    "email": "youngmayor.dev@gmail.com",
    "url": "https://mayrlabs.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MayR-Labs/mayrlabs-js.git",
    "directory": "packages/hello"
  },
  "bin": {
    "hello": "dist/hello.cjs"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "type": "commonjs",
  "main": "dist/hello.cjs",
  "module": "dist/hello.mjs",
  "exports": {
    ".": {
      "import": "./dist/hello.mjs",
      "require": "./dist/hello.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "pkg:dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "demo": "tsx src/hello.ts",
    "demo:dist": "node dist/hello.cjs",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^22.19.10",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "figlet": "^1.10.0",
    "open": "^10.2.0",
    "picocolors": "^1.1.1"
  }
}
